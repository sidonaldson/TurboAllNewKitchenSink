{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "format": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,yml,md}\"",
    "format:fix": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,yml,md}\"",
    "lint": "turbo run lint",
    "lint:errors": "turbo run lint:errors",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:cypress": "pnpm run --filter ./tests cypress",
    "test:cypress:ci": "pnpm run --filter ./tests cypress:ci"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.3.2",
    "turbo": "^1.9.3"
  },
  "packageManager": "pnpm@7.28.0"
}
